<h2>Show product</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @product.name %>
  </li>

  <li>
    <strong>Search key:</strong>
    <%= @product.search_key %>
  </li>

  <li>
    <strong>Vendor:</strong>
    <%= @product.vendor.name %>
  </li>

</ul>

<%= link "Edit", to: product_path(@conn, :edit, @product) %>
<%= link "Back", to: product_path(@conn, :index) %>
